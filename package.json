{
  "name": "ben-mclean-portfolio",
  "version": "0.1.0",
  "homepage": "http://BenMcLean981.github.io",
  "license": "UNLICENSED",
  "private": false,
  "dependencies": {
    "@fortawesome/fontawesome-common-types": "^6.2.1",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-brands-svg-icons": "^6.2.1",
    "@fortawesome/free-regular-svg-icons": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "prism-react-renderer": "^1.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-element-to-jsx-string": "^15.0.0",
    "react-router": "^6.6.1",
    "react-router-dom": "^6.6.1",
    "seedrandom": "^3.0.5",
    "web-vitals": "^2.1.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --all --watchAll",
    "test:coverage": "react-scripts test --all --watchAll=false --coverage || exit 0",
    "eject": "react-scripts eject",
    "analyze": "source-map-explorer 'build/static/js/*'",
    "predeploy": "yarn build && yarn test:coverage && yarn check-images",
    "deploy": "gh-pages -d build",
    "check-images": "if [ $(du -a public/images/* | sort -n -r | head -n 1 | cut -f1) -gt 200 ]; then exit 1; else exit 0; fi"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.18.6",
    "@craco/craco": "^6.4.3",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.17",
    "@types/react-dom": "^18.0.10",
    "autoprefixer": "^10.4.2",
    "gh-pages": "^3.2.3",
    "jest": "^29.3.1",
    "jest-environment-jsdom": "^29.3.1",
    "postcss": "^8.4.5",
    "react-scripts": "^5.0.1",
    "source-map-explorer": "^2.5.2",
    "tailwindcss": "^3.0.12",
    "ts-jest": "^29.0.3",
    "typescript": "^4.5.4"
  }
}
